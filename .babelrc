{
  "presets": [
    ["@babel/env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": [
        "transform-es2015-arrow-functions",
        "transform-object-rest-spread",
        [
            "@babel/plugin-proposal-pipeline-operator", 
            { "proposal": "minimal" }
        ]
  ]
}